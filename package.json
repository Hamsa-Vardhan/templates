{
  "name": "templates",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.5",
    "@types/react-dom": "^17.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "sass": "^1.34.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "rm -rf dist && parcel public/index.html",
    "build": "rm -rf dist && parcel build public/index.html"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
